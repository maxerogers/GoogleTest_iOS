# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

def datamanagement_pods
    pod 'Alamofire'
    pod 'AlamofireObjectMapper'
    pod 'ObjectMapper'
    pod 'SwiftyJSON'
end

target 'CommitteeTV' do
    use_frameworks!
    
    datamanagement_pods
end

target 'CommitteeTVTests' do
    use_frameworks!
  
    pod 'Quick'
    pod 'Nimble'
    datamanagement_pods
end

